print('1Q.Create one variable containing following type of data:\n(i)String') 
string = 'Ruthik'
print(string)
print(type(string))
print('(ii)List')
mylist=[1,2,8,'Ruthik',90.8,True]
print(mylist)
print(type(mylist))
print('(iii)Float')
flt=90.8
print(flt)
print(type(flt))
print('(iv)Tuple')
my_tuple=(2,4,8,'RG',2.2)
print(my_tuple)
print(type(my_tuple))
